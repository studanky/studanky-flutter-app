{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run (verbose)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "toolArgs": [
        "--verbose",
        "--dart-define-from-file=env_vars.json"
      ]
    },
    {
      "name": "Build IPA (verbose)",
      "request": "launch",
      "type": "dart",
      "customTool": "flutter",
      "customToolReplacesArgs": 1,
      "toolArgs": [
        "build",
        "ipa",
        "--verbose",
        "--obfuscate",
        "--split-debug-info=build/symbols",
        "--dart-define-from-file=env_vars.json"
      ]
    },
    {
      "name": "Build APK (verbose)",
      "request": "launch",
      "type": "dart",
      "customTool": "flutter",
      "customToolReplacesArgs": 1,
      "toolArgs": [
        "build",
        "apk",
        "--verbose",
        "--obfuscate",
        "--split-debug-info=build/symbols",
        "--dart-define-from-file=env_vars.json"
      ]
    }
  ]
}
